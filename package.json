{
  "name": "storybook",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf .dist",
    "clean:storybook": "rm -rf .storybook/src/*",
    "copy:storybook": "cp -r .dist/storybook/* .storybook/src",
    "nodemon": "nodemon --inspect --watch webpack.dev.config.js --watch webpack.common.config.js --exec \"webpack-dev-server --config webpack.dev.config.js\"",
    "dev": "yarn clean && yarn nodemon",
    "dev:storybook": "yarn build && yarn storybook",
    "build": "yarn clean && webpack --config webpack.prod.config.js --env.prod",
    "build:all": "yarn build && yarn storybook-build",
    "pre-build": "node ./scripts/pre_build",
    "post-build": "yarn clean:storybook && node ./scripts/post_build && yarn copy:storybook",
    "storybook": "start-storybook -s ./src",
    "storybook-build": "build-storybook -o .dist_ui"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@storybook/addon-docs": "^6.0.21",
    "@storybook/addon-viewport": "^6.0.21",
    "@storybook/html": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "clean-css-loader": "^2.0.0",
    "css-loader": "^4.3.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "file-loader": "^6.1.0",
    "group-css-media-queries-loader": "^3.0.2",
    "image-webpack-loader": "^7.0.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.7.1",
    "url-loader": "^4.1.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-ect-loader": "^1.0.5"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "bootstrap": "^4.5.2",
    "copy-webpack-plugin": "^6.1.0",
    "ect": "^0.5.9",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "gsap": "^3.5.1",
    "html-replace-webpack-plugin": "^2.5.6",
    "html-webpack-plugin": "^4.4.1",
    "jquery": "^3.5.1",
    "mini-css-extract-plugin": "^0.11.2",
    "popper.js": "^1.16.1",
    "scrollmagic": "^2.0.7",
    "stylelint-webpack-plugin": "^2.1.0",
    "swiper": "^6.2.0",
    "tailwindcss": "^1.8.10",
    "underscore": "^1.11.0",
    "webpack": "^4.44.1",
    "webpack-merge": "^5.1.4"
  }
}
